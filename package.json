{
  "name": "@dawnll/monorepo",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@9.5.0",
  "description": "dawnl monorepo",
  "author": "dawnl",
  "license": "ISC",
  "keywords": [],
  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "clean": "rimraf --glob \"packages/*/dist\"",
    "update": "tsx ./scripts/_update.ts",
    "release": "tsx ./scripts/bumpp.ts",
    "build": "pnpm clean && tsx ./scripts/build.ts",
    "publish:ci": "tsx ./scripts/publish.ts",
    "urp": "pnpm update && pnpm release && pnpm publish",
    "build:rollup": "pnpm --filter @dawnll/dlc build:rollup",
    "build:tsup": "pnpm --filter @dawnll/dlc build:tsup",
    "ppt": "pnpm --filter @dawnll/ppt dev",
    "dlc:tsx": "pnpm --filter @dawnll/dlc run tsx",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages"
  },
  "dependencies": {
    "md5": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@rollup/plugin-alias": "catalog:",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-json": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/pluginutils": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/node": "catalog:",
    "@unocss/eslint-plugin": "catalog:",
    "bumpp": "catalog:",
    "cac": "catalog:",
    "consola": "catalog:",
    "esbuild": "catalog:",
    "eslint": "catalog:",
    "fast-glob": "catalog:",
    "fs-extra": "catalog:",
    "gray-matter": "catalog:",
    "js-yaml": "catalog:",
    "markdown-it-mathjax3": "^4",
    "ofetch": "catalog:",
    "postcss": "^8",
    "rimraf": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-dts": "catalog:",
    "rollup-plugin-esbuild": "catalog:",
    "rollup-plugin-typescript2": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "simple-git": "catalog:",
    "tinyglobby": "^0.2.10",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "tslib": "catalog:",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-detect-duplicated-deps": "catalog:",
    "unplugin-externalize-deps": "catalog:",
    "vite": "catalog:",
    "vite-plugin-node-polyfills": "catalog:",
    "vitepress": "^1.5.0",
    "yaml": "catalog:"
  }
}
