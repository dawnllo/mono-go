{
  "name": "@dawnll/monorepo",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.29.3",
  "description": "dawnl monorepo",
  "author": "dawnl",
  "license": "ISC",
  "keywords": [],
  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "clean": "rimraf --glob \"packages/*/dist\"",
    "update": "tsx ./scripts/_update.ts",
    "release": "tsx ./scripts/bumpp.ts",
    "build": "pnpm clean && tsx ./scripts/build.ts",
    "publish:ci": "tsx ./scripts/publish.ts",
    "urp": "pnpm update && pnpm release && pnpm publish",
    "build:rollup": "pnpm --filter @dawnll/dlc build:rollup",
    "ppt": "pnpm --filter @dawnll/ppt dev",
    "dlc:tsx": "pnpm --filter @dawnll/dlc run tsx",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@codemod.com/cli-darwin-arm64": "catalog:catalog_tool",
    "@rollup/plugin-alias": "catalog:build",
    "@rollup/plugin-commonjs": "catalog:build",
    "@rollup/plugin-json": "catalog:build",
    "@rollup/plugin-node-resolve": "catalog:build",
    "@rollup/pluginutils": "catalog:build",
    "@types/fs-extra": "catalog:types",
    "@types/node": "catalog:types",
    "@types/prompts": "catalog:types",
    "@unocss/eslint-plugin": "catalog:lint",
    "bumpp": "catalog:script",
    "cac": "catalog:cli",
    "codemod": "catalog:catalog_tool",
    "consola": "catalog:cli",
    "esbuild": "catalog:build",
    "eslint": "catalog:lint",
    "fast-glob": "catalog:tool",
    "fs-extra": "catalog:tool",
    "prompts": "catalog:cli",
    "rimraf": "catalog:script",
    "rollup": "catalog:build",
    "rollup-plugin-dts": "catalog:build",
    "rollup-plugin-esbuild": "catalog:build",
    "rollup-plugin-typescript2": "catalog:build",
    "rollup-plugin-visualizer": "catalog:build",
    "simple-git": "catalog:tool",
    "tsx": "catalog:script",
    "typescript": "catalog:",
    "unplugin-auto-import": "catalog:build",
    "unplugin-detect-duplicated-deps": "catalog:build",
    "unplugin-externalize-deps": "catalog:build",
    "vite": "catalog:build",
    "vite-plugin-node-polyfills": "catalog:build",
    "vitepress": "catalog:ppt",
    "vitest": "catalog:test",
    "yaml": "catalog:tool"
  }
}
