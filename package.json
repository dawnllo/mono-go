{
  "name": "@dawnll/monorepo",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@9.1.4",
  "description": "dawnl monorepo",
  "author": "dawnl",
  "license": "ISC",
  "keywords": [],
  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "update": "tsx ./scripts/_update.ts",
    "release": "tsx ./scripts/bumpp.ts",
    "publish": "tsx ./scripts/publish.ts",
    "urp": "pnpm update && pnpm release && pnpm publish",
    "build:rollup": "pnpm --filter @dawnll/dlc build:rollup",
    "build:tsup": "pnpm --filter @dawnll/dlc build:tsup",
    "ppt": "pnpm --filter @dawnll/ppt dev",
    "dlc": "pnpm --filter @dawnll/dlc run dlc",
    "dlc:tsx": "pnpm --filter @dawnll/dlc run tsx",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/pluginutils": "^5.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.16.5",
    "@unocss/eslint-plugin": "0.59.0-beta.1",
    "bumpp": "^9.8.1",
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "ofetch": "^1.4.1",
    "rollup": "^4.21.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git": "^3.27.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.7.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-detect-duplicated-deps": "^2.0.0",
    "unplugin-externalize-deps": "^0.0.10",
    "vite": "^5.4.3",
    "vite-plugin-node-polyfills": "^0.21.0",
    "yaml": "^2.6.0"
  }
}
