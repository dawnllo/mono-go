{
  "name": "@dawnll/dlc",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@9.1.4",
  "description": "command dlc, a cli tool",
  "author": "dawnl",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./types/*": {
      "types": "./types/*"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./index.ts",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "dlc": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "build:rollup": "rollup --config rollup.config.ts --configPlugin esbuild",
    "tsx": "tsx ./index.ts"
  },
  "dependencies": {
    "@commander-js/extra-typings": "catalog:cli",
    "@types/inquirer": "catalog:types",
    "adm-zip": "catalog:tool",
    "chalk": "catalog:cli",
    "commander": "catalog:cli",
    "crypto-js": "catalog:tool",
    "download-git-repo": "catalog:tool",
    "ora": "catalog:cli",
    "prompts": "catalog:cli",
    "vite": "catalog:build"
  }
}
